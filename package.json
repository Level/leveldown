{
  "name": "leveldown",
  "description": "A low-level Node.js LevelDB binding",
  "version": "4.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/level/leveldown.git"
  },
  "homepage": "https://github.com/level/leveldown",
  "keywords": [
    "leveldb",
    "level"
  ],
  "main": "leveldown.js",
  "dependencies": {
    "abstract-leveldown": "~6.0.0",
    "bindings": "~1.3.0",
    "fast-future": "~1.0.2",
    "nan": "~2.11.0",
    "prebuild-install": "^5.0.0"
  },
  "devDependencies": {
    "async": "^2.0.1",
    "coveralls": "^3.0.2",
    "delayed": "~1.0.1",
    "du": "~0.1.0",
    "level-concat-iterator": "^2.0.0",
    "mkfiletree": "~1.0.1",
    "monotonic-timestamp": "~0.0.8",
    "nyc": "^12.0.2",
    "optimist": "~0.6.1",
    "prebuild": "^8.0.0",
    "prebuild-ci": "^2.0.0",
    "readfiletree": "~0.0.1",
    "remark-cli": "^5.0.0",
    "remark-github": "^7.0.3",
    "rimraf": "^2.6.1",
    "slump": "~2.0.0",
    "standard": "^12.0.0",
    "tape": "^4.5.1",
    "tempy": "^0.2.1",
    "uuid": "^3.2.1",
    "verify-travis-appveyor": "^3.0.0"
  },
  "scripts": {
    "install": "prebuild-install || git submodule update && node-gyp rebuild",
    "test": "standard && verify-travis-appveyor && nyc tape test/*-test.js && prebuild-ci",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "rebuild": "prebuild --compile",
    "prebuild": "prebuild --all --strip --verbose",
    "changelog": "remark CHANGELOG.md -o"
  },
  "remarkConfig": {
    "plugins": [
      "remark-github"
    ]
  },
  "license": "MIT",
  "gypfile": true,
  "engines": {
    "node": ">=6"
  }
}
