{
  "name": "leveldown",
  "description": "A low-level Node.js LevelDB binding",
  "version": "5.0.0-0",
  "repository": {
    "type": "git",
    "url": "https://github.com/level/leveldown.git"
  },
  "homepage": "https://github.com/level/leveldown",
  "keywords": [
    "leveldb",
    "level"
  ],
  "main": "leveldown.js",
  "dependencies": {
    "abstract-leveldown": "~6.0.0",
    "bindings": "~1.3.0",
    "fast-future": "~1.0.2",
    "napi-macros": "~1.8.1",
    "node-gyp-build": "~3.7.0"
  },
  "devDependencies": {
    "async": "^2.0.1",
    "coveralls": "^3.0.2",
    "delayed": "~1.0.1",
    "du": "~0.1.0",
    "hallmark": "~0.1.0",
    "level-concat-iterator": "^2.0.0",
    "mkfiletree": "~1.0.1",
    "monotonic-timestamp": "~0.0.8",
    "node-gyp": "^3.8.0",
    "nyc": "^12.0.2",
    "optimist": "~0.6.1",
    "prebuildify": "github:ahdinosaur/prebuildify",
    "prebuildify-ci": "^1.0.4",
    "prebuildify-cross": "^2.0.3",
    "readfiletree": "~0.0.1",
    "rimraf": "^2.6.1",
    "slump": "~3.0.0",
    "standard": "^12.0.0",
    "tape": "^4.5.1",
    "tempy": "^0.2.1",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "install": "node-gyp-build",
    "test": "standard && nyc tape test/*-test.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "rebuild": "node-gyp rebuild",
    "prebuild": "prebuildify -t 8.14.0 -t electron@3.0.0 --napi --strip",
    "prebuild:cross:android-arm": "prebuildify-cross --target android-arm",
    "prebuild:cross:android-arm64": "prebuildify-cross --target android-arm64",
    "prebuild:cross:linux-armv6": "prebuildify-cross --target linux-armv6",
    "prebuild:cross:linux-armv7": "prebuildify-cross --target linux-armv7",
    "prebuild:cross:linux-arm64": "prebuildify-cross --target linux-arm64",
    "prebuild:cross:linux-x64": "prebuildify-cross --target linux-x64",
    "prebuild:cross:windows-x64": "prebuildify-cross --target windows-x64",
    "download-prebuilds": "prebuildify-ci download",
    "hallmark": "hallmark --fix UPGRADING.md CHANGELOG.md"
  },
  "license": "MIT",
  "gypfile": true,
  "engines": {
    "node": ">=8.6.0"
  }
}
